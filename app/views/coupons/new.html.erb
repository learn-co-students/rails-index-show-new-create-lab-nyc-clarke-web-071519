<h1>New Coupon Form</h1>

<%= form_tag coupons_path do %>

    <%= label_tag "coupon[store]", "Store:"%><br>
    <%= text_field_tag "coupon[store]" %><br><br>

    <%= label_tag "coupon[coupon_code]", "Coupon Code:"%><br>
    <%= text_field_tag "coupon[coupon_code]" %><br><br>

    <%= submit_tag "Submit Coupon" %>
<% end%>